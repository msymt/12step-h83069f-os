/* OUTPUT_FORMAT("elf32-h8300") */
OUTPUT_ARCH(h8300h)
ENTRY("_start")

SECTIONS
{
	. = 0x0;
	
	/* set 0x000000 ~ 0x0000ff */
	.vectors : {
		vector.o(.data)
	}

	.text : {
		*(.text)
	}

	.rodata : {
		*(.strings)
		*(.rodata)
		*(.rodata.*)
	}

	.data : {
		*(.data)
	}

	.bss : {
		*(.bss)
		*(COMMON)
	}
}
